+	S2LOEQ	data2, data2, #8
+	beq	.Lstrcmp_tail
+	sub	result, r2, result
+	ldmfd	sp!, {r4, r5}
+	.cfi_restore 4
+	.cfi_restore 5
+	.cfi_def_cfa_offset 0
+	RETURN
+	.cfi_endproc
