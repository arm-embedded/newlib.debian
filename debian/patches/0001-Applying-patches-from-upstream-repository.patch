+	.cfi_def_cfa_offset 4
+	.cfi_offset 4, -4
+	mov	magic1, #1
+	orr	magic1, magic1, magic1, lsl #8
+	orr	magic1, magic1, magic1, lsl #16
+	.p2align	2
+4:
+	sub	syndrome, data1, magic1
+	cmp	data1, data2
